@article{Ralchenko2008,
title = {Electron-impact excitation and ionization cross sections for ground state and excited helium atoms},
journal = {Atomic Data and Nuclear Data Tables},
volume = {94},
number = {4},
pages = {603-622},
year = {2008},
issn = {0092-640X},
